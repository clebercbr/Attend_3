begin
  Application.Title := 'OT-Soft Attend';
  Application.HelpFile := 'Help.hlp';
  FSplash := TFSplash.Create(Application);
  FSplash.Gauge1.Progress := 00;
  FSplash.Show;
  Application.CreateForm(TFInicial, FInicial);
  FSplash.Gauge1.Progress := 10;
  FSplash.Update;
  Application.CreateForm(TFCadObse, FCadObse);
  FSplash.Gauge1.Progress := 20;
  FSplash.Update;
  Application.CreateForm(TFFuncoes, FFuncoes);
  FSplash.Gauge1.Progress := 40;
  FSplash.Update;
  Application.CreateForm(TFTabTele, FTabTele);
  FSplash.Gauge1.Progress := 70;
  FSplash.Update;
  Application.CreateForm(TFUltPedi, FUltPedi);
  FSplash.Gauge1.Progress := 80;
  FSplash.Update;
  Application.createForm(TFFinPedi,FFinPedi);
  FSplash.Gauge1.Progress := 90;
  FSplash.Update;
  Application.createForm(TFCadPedi,FCadPedi);
  FSplash.Gauge1.Progress := 100;
  FSplash.Update;
  Application.Initialize;
  FSplash.Hide;
  FSplash.Free;
  Application.Run;
//-----------------------------------------------------------------------------------------------
end.
